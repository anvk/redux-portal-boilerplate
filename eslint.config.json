{
  "ecmaFeatures": {
    "jsx": true
  },
  "env": {
    "browser": true,
    "node": true,
    "jquery": true
  },
  "rules": {

    "no-trailing-spaces": 1,
    "eol-last": 1,

    "no-alert": 1,
    "no-lone-blocks": 1,
    "no-undef": 1
  },
  "globals": {
    "jQuery": true,
    "$": true
  }
}