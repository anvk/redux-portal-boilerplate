{
  "name": "redux-portal-boilerplate",
  "version": "0.0.11",
  "author": "anvk",
  "description": "A boilerplate to create a portal website using Redux & Bootstrap & Redux Dev Tools Extension.",
  "contributors": [
    {
      "name": "Alexey Novak",
      "email": "alexey.novak.mail@gmail.com"
    }
  ],
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint src",
    "dev": "webpack-dev-server -d --config webpack/hot.dev.config",
    "build:prod": "npm run clean && webpack -p --progress --verbose --colors --display-error-details --config ./webpack/prod.config",
    "build:dev": "npm run clean && webpack -d --progress --verbose --colors --display-error-details --config ./webpack/dev.config",
    "test": "mocha test/helpers/browser.js test/ --compilers js:babel-register --recursive --require ignore-styles",
    "test:watch": "npm test -- --watch"
  },
  "directories": {
    "test": "test",
    "dist": "dist",
    "src": "src"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:anvk/redux-portal-boilerplate.git"
  },
  "keywords": [
    "redux",
    "boilerplate",
    "portal",
    "bootstrap",
    "webpack",
    "material-ui"
  ],
  "dependencies": {
    "babel-polyfill": "6.13.0",
    "bootstrap": "3.3.7",
    "jquery": "3.1.0",
    "ladda": "1.0.0",
    "material-ui": "0.15.4",
    "promise": "7.1.1",
    "query-params": "0.0.1",
    "react": "15.3.1",
    "react-dom": "15.3.1",
    "react-gravatar": "2.4.5",
    "react-ladda": "4.0.5",
    "react-redux": "4.4.5",
    "react-router": "2.7.0",
    "react-router-redux": "4.0.5",
    "react-tap-event-plugin": "1.0.0",
    "react-tooltip": "3.1.6",
    "redux": "3.6.0",
    "redux-thunk": "2.1.0",
    "superagent": "2.2.0",
    "toastr": "2.1.2"
  },
  "devDependencies": {
    "autoprefixer-loader": "3.1.0",
    "babel-eslint": "5.0.0",
    "babel-loader": "6.2.0",
    "babel-preset-es2015": "6.1.18",
    "babel-preset-react": "6.1.18",
    "babel-preset-stage-0": "6.3.13",
    "babel-register": "6.14.0",
    "css-loader": "0.23.0",
    "enzyme": "2.4.1",
    "eslint": "2.2.0",
    "eslint-config-airbnb": "6.0.2",
    "eslint-plugin-react": "4.1.0",
    "expect": "1.14.0",
    "file-loader": "0.8.5",
    "html-webpack-plugin": "2.9.0",
    "ignore-styles": "5.0.1",
    "jsdom": "9.5.0",
    "jsdom-global": "2.1.0",
    "json-loader": "0.5.4",
    "less-loader": "2.2.2",
    "mocha": "2.4.5",
    "proxyquire": "1.7.10",
    "redux-immutable-state-invariant": "1.2.0",
    "redux-logger": "2.3.2",
    "redux-mock-store": "1.2.1",
    "rimraf": "2.3.4",
    "sass-loader": "3.1.2",
    "style-loader": "0.13.0",
    "url-loader": "0.5.7",
    "webpack": "1.12.6",
    "webpack-dev-middleware": "1.5.1",
    "webpack-dev-server": "1.14.0",
    "webpack-hot-middleware": "2.7.1"
  },
  "engine": "node >= 0.10.x",
  "private": true,
  "license": "MIT"
}
