{
  "name": "redux-portal-boilerplate",
  "version": "0.0.6",
  "author": "anvk",
  "description": "A boilerplate to create a portal website using Redux & Bootstrap & Redux Dev Tools Extension.",
  "contributors": [
    {
      "name": "Alexey Novak",
      "email": "alexey.novak.mail@gmail.com"
    }
  ],
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint src",
    "dev": "webpack-dev-server -d --config webpack/hot.dev.config",
    "build:prod": "npm run clean && webpack -p --progress --verbose --colors --display-error-details --config ./webpack/prod.config",
    "build:dev": "npm run clean && webpack -d --progress --verbose --colors --display-error-details --config ./webpack/dev.config",
    "test": "karma start"
  },
  "directories": {
    "test": "test",
    "dist": "dist",
    "src": "src"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:anvk/redux-portal-boilerplate.git"
  },
  "keywords": [
    "redux",
    "boilerplate",
    "portal",
    "bootstrap",
    "webpack"
  ],
  "dependencies": {
    "bootstrap": "3.3.5",
    "history": "1.17.0",
    "jquery": "2.1.4",
    "promise": "7.1.1",
    "react": "0.14.2",
    "react-dom": "0.14.2",
    "react-redux": "4.4.0",
    "react-router": "2.0.0",
    "react-router-redux": "3.0.0",
    "redux": "3.3.1",
    "redux-thunk": "1.0.3"
  },
  "devDependencies": {
    "autoprefixer-loader": "3.1.0",
    "babel-loader": "6.2.0",
    "babel-preset-es2015": "6.1.18",
    "babel-preset-react": "6.1.18",
    "babel-preset-stage-0": "6.3.13",
    "css-loader": "0.23.0",
    "eslint": "2.1.0",
    "eslint-plugin-react": "3.16.1",
    "expect": "1.14.0",
    "express": "4.13.4",
    "file-loader": "0.8.5",
    "html-webpack-plugin": "2.9.0",
    "jasmine-core": "2.4.1",
    "json-loader": "0.5.4",
    "karma": "0.13.21",
    "karma-chrome-launcher": "0.2.2",
    "karma-mocha": "0.2.2",
    "karma-webpack": "1.7.0",
    "less-loader": "2.2.2",
    "mocha": "2.4.5",
    "redux-immutable-state-invariant": "1.2.0",
    "redux-logger": "2.3.2",
    "rimraf": "2.3.4",
    "sass-loader": "3.1.2",
    "style-loader": "0.13.0",
    "url-loader": "0.5.7",
    "webpack": "1.12.6",
    "webpack-dev-middleware": "1.5.1",
    "webpack-dev-server": "1.14.0",
    "webpack-hot-middleware": "2.7.1"
  },
  "engine": "node >= 0.10.x",
  "private": true,
  "license": "MIT"
}
